                                                <div class="card-title">
                                                        <%=ppdvideoinfo[0].snippet.title%>
                                                </div>
                                                <p class="card-text">
                                                        <%=ppdvideoinfo[0].snippet.description%>
                                                </p>
                                                <p class="card-text">from channel: <%=ppdvideoinfo[0].snippet.channelTitle%></p>
                                        
                                                <button data-id="<%=ppdvideoinfo[0].id.videoId%>" class="videowatched btn btn-primary"> click when watched</button>
                                                <button data id="<%=ppdvideoinfo[0].id.videoId%>" class="videosaved btn btn-primary"> click to save</button>
                                                <p class="hidden" id="videoSaved">Video Saved!</p>
                                                <p class="hidden" id="alreadySaved">Video Already Saved</p>
                                        </div>
                                </div>
                        </div>
                </div>
